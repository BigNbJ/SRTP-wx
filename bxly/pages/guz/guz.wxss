/* pages/guz/guz.wxss */

.btn{
  width: initial!important;
  height: 70rpx;
  padding: 0 50rpx;
  line-height: 70rpx;
  font-size: 24rpx; 
  border: 1px solid #dcdcdc;
  background: #fff;
  border-radius:100rpx;
  margin: 5rpx!important;
  text-align: center;
  float: left;
}
.btn-selected{
  border: 1px solid #ffea00;
  background: #ffea00;
  /* color: #fff; */
}
.comment-btn{
  overflow: hidden;
}
.nimensc_se{
  padding: 20rpx;
  overflow: hidden;
}

.nimensc_se .biaot{
  padding: 10rpx;
  padding-bottom: 25rpx;
}

.nimensc_se .biaodan{
  padding: 30rpx 0;
}

.nimensc_se .biaodan textarea{
  border: solid 1px #eee;
  width: 96%;
  padding: 2%;
  border-radius:5rpx;
  line-height: 30rpx;
}

/*  */


.q-image-wrap{
  width: 150rpx;
  height: 150rpx;
  border: 1px solid #f0f0f0;
  float: left;
  margin-right: 20rpx;
  position: relative;
  overflow: hidden;
}
.q-image-remover{
  width: 50rpx;
  height: 50rpx;
  border-radius: 100%;
  background: #2e82ff;
  position: absolute;
  top:0;
  right:0;
  background:#444 url(data:image/png;base64,) no-repeat center;
  background-size: 24rpx;
}
.q-image-remover icon{
  position: absolute;
  left:12rpx;
  bottom:12rpx;
  color:#fff;
}
 
.uploadImgBtn{
  width: 150rpx;
  height: 150rpx;
  text-align: center;
  line-height: 140rpx;
  border:1rpx solid #f0f0f0;
  float: left;
}
.uploadImgBtn icon{
  font-size: 80rpx;
  color: #999;
} 

/* 上传资料 */
.waikuang_shangc{
  padding:20rpx;
  overflow: hidden;
  padding-bottom: 40rpx;
}
.waikuang_shangc .pf-type{
  padding-bottom: 20rpx;
} 

.q-image-wrap image{
  width: 150rpx;
  height: 150rpx;
}

.waikuang_shangc .coior_Se{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 60rpx;
  width: 150rpx;
  height: 150rpx;
}

.tijiao{
  width:96%!important;
  margin: 2%!important;
  line-height:58rpx;
}


/* / */

.nimen_shaom{
  padding: 20rpx;
  position: relative;
}

.nimen_shaom input{
  border: solid #eee 1px;
  padding: 15rpx 15rpx; 
   border-radius: 8rpx;
}

.nimen_shaom .erweim{
  width: 65rpx;
  height: 65rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 55rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  right:30rpx;
  margin: auto;
  z-index: 1;
  cursor: pointer;
}